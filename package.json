{
  "name": "JackMarvelsApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:services": "jest __tests__/services/",
    "test:screens": "jest __tests__/screens/",
    "test:utils": "jest __tests__/utils/",
    "test:components": "jest __tests__/components/",
    "test:stores": "jest __tests__/stores/",
    "test:hooks": "jest __tests__/hooks/",
    "test:all": "node __tests__/run-tests.js",
    "test:report": "jest --coverage --coverageReporters=text --coverageReporters=html --coverageReporters=json",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "postinstall": "if [ -d 'ios' ] && command -v bundle >/dev/null 2>&1; then cd ios && bundle exec pod install || true; elif [ -d 'ios' ] && command -v pod >/dev/null 2>&1; then cd ios && pod install || true; fi",
    "clean": "rm -rf node_modules && yarn install",
    "clean:metro": "npx react-native start --reset-cache",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace JackMarvelsApp.xcworkspace -scheme JackMarvelsApp -configuration Release -destination generic/platform=iOS -archivePath JackMarvelsApp.xcarchive archive"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.52",
    "@gluestack-ui/config": "^1.1.18",
    "@gluestack-ui/themed": "^1.1.18",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/elements": "^1.3.30",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.3.29",
    "@tanstack/react-query": "^5.44.0",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "lottie-react-native": "5.1.6",
    "lucide-react-native": "^0.515.0",
    "moti": "^0.30.0",
    "react": "18.3.1",
    "react-i18next": "^15.7.1",
    "react-native": "0.75.3",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-device-info": "^11.1.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-image-picker": "^7.1.2",
    "react-native-indicators": "^0.17.0",
    "react-native-keychain": "^8.1.3",
    "react-native-markdown-display": "^7.0.2",
    "react-native-otp-textinput": "^1.1.7",
    "react-native-permissions": "^4.1.5",
    "react-native-push-notification": "^8.1.1",
    "react-native-razorpay": "2.3.1",
    "react-native-reanimated": "^3.12.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.12.0",
    "react-native-webview": "^13.6.4",
    "react-native-youtube-iframe": "^2.4.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.3",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4",
    "yarn": "^1.22.22"
  },
  "engines": {
    "node": ">=18"
  }
}
